<div class="container">
  <div class="row">
    <div class="col-12">
      <h3 class="my-5">ジャンル編集</h3>
      
      <%= form_with model: @genre, url: admin_genre_path do |f| %>
        <%= f.label 'ジャンル名' %>
        <%= f.text_field :name, autofocus: true, autocomplete: "ジャンル名" %>
        <%= f.submit "変更を保存", :class => "btn btn-success" %>
      <% end %>
      
    </div>
  </div>
</div>
